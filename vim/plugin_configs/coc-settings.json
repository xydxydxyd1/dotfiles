  "suggest.noselect": true,
  "list.insertMappings": {
    "<C-j>": "command:CocNext",
    "<C-k>": "command:CocPrev"
  },
  "languageserver": {
    "csharp": {
      "command": "csharp-ls",
      "rootPatterns": ["*.sln", "git"],
      "filetypes": ["cs", "csharp"]
    }
  },
  "pyright.inlayHints.variableTypes": false,
  "pyright.inlayHints.functionReturnTypes": false,
  "pyright.inlayHints.parameterTypes": false
}
